{
  "name": "express-testing",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config --exec babel-node ./src/server.js",
    "start": "node ./build/server.js",
    "build": "babel src --out-dir build",
    "lint": "eslint ./",
    "test": "jest --setupFiles dotenv/config",
    "coveralls": "jest --setupFiles dotenv/config --coverage && coveralls < coverage/lcov.info",
    "knex": "knex --knexfile=./src/lib/knex/knexfile.js --esm",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed": "npm run knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "knex": "^0.95.11",
    "morgan": "^1.10.0",
    "npm": "^7.23.0",
    "pg": "^8.7.1",
    "regenerator-runtime": "^0.13.9",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "supertest": "^6.1.5"
  }
}
